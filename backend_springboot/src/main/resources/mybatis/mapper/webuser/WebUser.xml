<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.devh.springboot.webadmin.webuser.mapper.WebUserMapper">

    <select id="selectByUserId" resultType="com.devh.springboot.webadmin.webuser.model.WebUser" parameterType="java.lang.String">
        SELECT *
        FROM   WEB_USER
        WHERE  USER_ID = #{userId}
    </select>
  
</mapper>