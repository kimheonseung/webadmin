<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.devh.springboot.webadmin.monitoring.mapper.ChartMapper">
    <select id="selectChartById" parameterType="java.lang.Integer" resultType="com.devh.springboot.webadmin.monitoring.model.Chart">
        SELECT *
        FROM   CHART
        WHERE  ID = ${chartId}
    </select>
</mapper>